<gui>
  <checkbox>
    <name>draw_rigid_objects_faces</name>
    <descr>Draw Face Mesh of Rigid Objects</descr>
    <key>f</key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>draw_rigid_objects_edges</name>
    <descr>Draw Edges Mesh of Rigid Objects</descr>
    <key>e</key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>draw_forcefield</name>
    <descr>Draw All ForceFields in Workspace</descr>
    <key></key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>draw_wrenchfield</name>
    <descr>Draw All Wrenches for each Link</descr>
    <key></key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_forceellipsoid</name>
    <descr>Draw Force Ellipsoid</descr>
    <key></key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_distance_robot_terrain</name>
    <descr>Draw Distance Robot Terrain</descr>
    <key></key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>draw_com_path</name>
    <descr>Draw Center Of Mass Path</descr>
    <key></key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_planner_bounding_box</name>
    <descr>Draw R3 Bounding Box of Planner</descr>
    <key>b</key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_path_space</name>
    <descr>Draw Path Space</descr>
    <key>g</key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_path_sweptvolume</name>
    <descr>Draw Swept Volume</descr>
    <key>l</key>
    <active>0</active>
  </checkbox>

  <checkbox>
    <name>draw_roadmap</name>
    <descr>Draw Roadmap</descr>
    <active>1</active>
  </checkbox>

  <checkbox>
    <name>draw_play_path</name>
    <descr>Replay Path</descr>
    <key>5</key>
    <active>0</active>
  </checkbox>
  <button>
    <name>toggle_mode</name>
    <descr>Toggle Mode</descr>
    <key>m</key>
  </button>
  <button>
    <name>save_current_path</name>
    <descr>Save Current Path</descr>
    <key>0</key>
  </button>

  <hotkey>
    <name>load_current_path</name>
    <descr>Load Current Path</descr>
    <key>9</key>
  </hotkey>

  <hotkey>
    <name>reset</name>
    <descr>Reset</descr>
    <key>r</key>
  </hotkey>
  <hotkey>
    <name>simulate</name>
    <descr>Simulate</descr>
    <key>s</key>
  </hotkey>
  <hotkey>
    <name>advance</name>
    <descr>Advance by one step</descr>
    <key>a</key>
  </hotkey>
  <hotkey>
    <name>make_screenshot</name>
    <descr>Screenshot</descr>
    <key>S</key>
  </hotkey>
  <hotkey>
    <name>print_robot_info</name>
    <descr>Print Robot</descr>
    <key>i</key>
  </hotkey>
  <hotkey>
    <name>draw_robot_previous</name>
    <descr>Draw Previous Robot</descr>
    <key>N</key>
  </hotkey>
  <hotkey>
    <name>draw_robot_next</name>
    <descr>Draw Next Robot</descr>
    <key>M</key>
  </hotkey>
  <checkbox>
    <name>draw_robot</name>
    <descr>Draw Original Robot</descr>
    <key></key>
    <active>0</active>
  </checkbox>
  <hotkey>
    <name>draw_roadmap_swathvolume</name>
    <descr>Draw Roadmap Swathvolume</descr>
    <key>k</key>
  </hotkey>

  <checkbox>
    <name>roadmap_visualize_level_0</name>
    <descr>Roadmap Vis Level 0</descr>
    <key>6</key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>roadmap_visualize_level_1</name>
    <descr>Roadmap Vis Level 1</descr>
    <key>7</key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>roadmap_visualize_level_2</name>
    <descr>Roadmap Vis Level 2</descr>
    <key>8</key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_roadmap_vertices</name>
    <descr>Draw Roadmap Vertices</descr>
    <key>1</key>
    <active>1</active>
  </checkbox>
  <checkbox>
    <name>draw_roadmap_edges</name>
    <descr>Draw Roadmap Edges</descr>
    <key>2</key>
    <active>0</active>
  </checkbox>
  <checkbox>
    <name>draw_roadmap_volume</name>
    <descr>Draw Roadmap Volume</descr>
    <key>3</key>
    <active>1</active>
  </checkbox>

  <property name="draw_path" enabled="0"/>
  <property name="draw_path_unsmoothed" enabled="0"/>

  <property name="draw_axes" enabled="0"/>
  <property name="draw_cover_vertices" enabled="1"/>
  <property name="draw_robot_zero_position" enabled="1"/>
  <property name="planner_draw_start_configuration" enabled="1"/>
  <property name="planner_draw_goal_configuration" enabled="1"/>

</gui>

