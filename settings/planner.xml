<!-- Default Planner Settings -->
<planner>

  <algorithm dynamic="1" name="ompl:est"/>
  <algorithm dynamic="1" name="ompl:sst"/>
  <algorithm dynamic="1" name="ompl:pdst"/>
  <algorithm dynamic="1" name="ompl:rrt"/>
  <algorithm dynamic="1" name="ompl:kpiece"/>
  <algorithm dynamic="1" name="benchmark:all_dynamic"/>

  <algorithm name="hierarchy:qmp_sufficient"/>
  <!-- <algorithm name="hierarchy:qmp_connect"/> -->
  <algorithm name="ompl:rrtconnect"/>
  <!-- <algorithm name="ompl:prm"/> -->

  <!-- <algorithm name="ompl:prm"/> -->
  <!-- <algorithm name="hierarchy:qmp_connect_rrt"/> -->
  <!-- <algorithm name="hierarchy:qmp_connect"/> -->

  <!--
  <algorithm name="qmp_connect"/>
  <algorithm name="qmp_connect_cover"/>
  <algorithm name="ompl:rrt"/>
  <algorithm name="benchmark:best_geometric"/>
  <algorithm name="benchmark:all_geometric"/>
-->

  <maxplanningtime>1</maxplanningtime>      <!-- runtime in (s) --> 
  <smoothPath>0</smoothPath>                  <!-- 0: no smoothing, 1: smoothing      -->
  <pathSpeed>1</pathSpeed>                    <!-- <1: slower, 1: default, >1: faster -->
  <sampler name="uniform"/>                   <!-- uniform|gaussian|minimum_clearance|maximum_clearance|obstacle_based|bridge_test -->
  <timestep min="0.01" max="0.1"/>
  <epsilongoalregion>0.01</epsilongoalregion>
  <freeFloating>0</freeFloating>
  <kinodynamic>0</kinodynamic> 
  <enableSufficiency>0</enableSufficiency>
</planner>
