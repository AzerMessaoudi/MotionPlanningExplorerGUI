<?xml version="1.0"?>
<robot name="sentinel">

  <link name="base_link">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.004" ixy="0.001" ixz="0" iyy="0.006" iyz="0" izz="0.007"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius=0.3/>
      </geometry>
    </collision>
  </link>

<!-- ##########################################################################
  ## Head Link 
########################################################################## -->
  <link name="head">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.004" ixy="0.001" ixz="0" iyy="0.006" iyz="0" izz="0.007"/>
    </inertial>
    <collision>
      <origin rpy="0 1.54 0" xyz="-0.15 0 0"/>
      <geometry>
        <cylinder length="0.3" radius="0.3"/>
      </geometry>
    </collision>
  </link>
  <joint name="headbase" type="fixed">
    <origin rpy="0 -0 0" xyz="0 0 0"/>
    <parent link="base_link"/>
    <child link="head"/>
  </joint>
<!-- ##########################################################################
  ## Branch 1
########################################################################## -->
  <link name="l11">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.004" ixy="0.001" ixz="0" iyy="0.006" iyz="0" izz="0.007"/>
    </inertial>
    <collision>
      <origin rpy="0 1.54 0" xyz="-0.1 0 0"/>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
  </link>
  <joint name="j11" type="fixed">
    <origin rpy="0 0 0" xyz="-0.3 0 0"/>
    <parent link="head"/>
    <child link="l11"/>
  </joint>
  <link name="l12">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.004" ixy="0.001" ixz="0" iyy="0.006" iyz="0" izz="0.007"/>
    </inertial>
    <collision>
      <origin rpy="0 1.54 0" xyz="-0.5 0 0"/>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
    </collision>
  </link>
  <joint name="j12" type="revolute">
    <origin rpy="0 0 0" xyz="-0.3 0 0"/>
    <parent link="l11"/>
    <child link="l12"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="10" friction="0"/>

    <limit lower="-1.57" upper="1.57" effort="100" velocity="1000"/>
  </joint>




</robot>
